{
  "name": "submit_java",
  "version": "1.0.1",
  "description": "Build an Extension!",
  "permissions": [
    "https://www.acmicpc.net/submit/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.acmicpc.net/submit/*",
        "https://swexpertacademy.com/main/solvingProblem/solvingProblem.do",
        "http://www.jungol.co.kr/theme/jungol/submitpage.php?*"
      ],
      "js": [
        "inject.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "boj.js",
    "swea.js",
    "jungol.js"
  ],
  "manifest_version": 2
}